{
  "name": "crmgrow",
  "version": "0.1.6",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --host 127.0.0.1 --port=4200",
    "host": "ng serve --host 127.0.0.1 --port 4200",
    "build": "ng build --configuration production",
    "test": "ng test --progress=true",
    "lint": "tsc --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "e2e": "ng e2e",
    "postinstall": "patch-package",
    "build:prod-sspa": "ng build crmgrow --configuration prod-sspa && mv dist/prod-sspa/main.js dist/prod-sspa/redx-crm-client.js",
    "build:stage-sspa": "ng build crmgrow --configuration stage-sspa && mv dist/stage-sspa/main.js dist/stage-sspa/redx-crm-client.js",
    "build:dev-sspa": "ng build crmgrow --configuration dev-sspa && mv dist/dev-sspa/main.js dist/stage-sspa/redx-crm-client.js",
    "start:dev-sspa": "ng serve --project crmgrow --host=127.0.0.1 --port=4200 --live-reload false --configuration dev-sspa",
    "normalize:assets": "node ./src/scripts/normalize-assets.js --path=src --save",
    "preview:assets": "node ./src/scripts/normalize-assets.js --path=src",
    "prepare": "husky install"
  },
  "private": true,
